{
  "name": "aquawise-api",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node --max-old-space-size=1024 dist/src/main.js",
    "dev": "NODE_ENV=dev nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=prod node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typeorm": "npm run build && npx typeorm -d dist/database/data-source.js",
    "migration:generate": "npm run typeorm -- migration:generate",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "seed": "ts-node database/seeding/seed.ts",
    "drop": "ts-node database/seeding/drop.ts"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.2.1",
    "@hapi/joi": "^17.1.1",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^9.4.0",
    "@nestjs/elasticsearch": "^9.0.0",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/typeorm": "^9.0.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express-session": "^1.17.3",
    "firebase-admin": "^11.8.0",
    "handlebars": "^4.7.7",
    "jwks-rsa": "^3.0.1",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "typeorm": "^0.3.14",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.4.2",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^9.4.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.5",
    "@types/hapi__joi": "^17.1.8",
    "@types/jest": "29.5.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.1",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.5.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.6",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "4.8.4"
  }
}
